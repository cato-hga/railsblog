<div class="page_wrapper">
<h2><%= @post.title %></h2>

<p>
  Submitted <%= time_ago_in_words(@post.created_at) %> Ago
  | <%= link_to 'Edit?', edit_post_path(@post) %> | <%= link_to 'Delete?', post_path(@post), method: :delete, data: { confirm: 'Uh oh! Are you sure you would like to delete post?'} %>
</p>

<p>
  <%= @post.body %>
</p>
</div>